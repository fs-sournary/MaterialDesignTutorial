<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="@dimen/dp_24"
    android:height="@dimen/dp_24"
    android:tint="?attr/colorControlNormal"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <group
        android:name="@string/collapse_group_name"
        android:translateX="12"
        android:translateY="9">

        <group
            android:name="@string/collapse_left_group_name"
            android:rotation="45">

            <group android:translateY="3">
                <path
                    android:fillColor="@android:color/white"
                    android:pathData="@string/collapse_left_path_data" />
            </group>
        </group>

        <group
            android:name="@string/collapse_right_group_name"
            android:rotation="135">

            <group android:translateY="-3">
                <path
                    android:fillColor="@android:color/white"
                    android:pathData="@string/collapse_right_path_data" />
            </group>
        </group>
    </group>
</vector>
