<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_fingerprint_on">

    <target android:name="@string/fingerprint_ridge_5_path_name">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:duration="20"
                    android:interpolator="@android:interpolator/linear"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="0.0"
                    android:valueType="floatType" />

                <objectAnimator
                    android:duration="180"
                    android:interpolator="@anim/interpolator_fingerprint_on_to_off_1"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="1.0"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>

    <target android:name="@string/fingerprint_ridge_4_path_name">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:duration="10"
                    android:interpolator="@android:interpolator/linear"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="0.0"
                    android:valueType="floatType" />

                <objectAnimator
                    android:duration="160"
                    android:interpolator="@anim/interpolator_fingerprint_on_to_off_1"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="1.0"
                    android:valueType="floatType" />
            </set>
        </aapt:attr>
    </target>

    <target android:name="@string/fingerprint_ridge_3_path_name">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="190"
                android:interpolator="@anim/interpolator_fingerprint_on_to_off_1"
                android:propertyName="trimPathEnd"
                android:valueFrom="1.0"
                android:valueTo="0.0"
                android:valueType="floatType" />
        </aapt:attr>
    </target>

    <target android:name="@string/fingerprint_ridge_2_path_name">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="140"
                android:interpolator="@anim/interpolator_fingerprint_on_to_off_0"
                android:propertyName="trimPathEnd"
                android:valueFrom="1.0"
                android:valueTo="0.0"
                android:valueType="floatType" />
        </aapt:attr>
    </target>

    <target android:name="@string/fingerprint_ridge_1_path_name">
        <aapt:attr name="android:animation">
            <set android:ordering="sequentially">
                <objectAnimator
                    android:duration="60"
                    android:interpolator="@android:interpolator/linear"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="0.0"
                    android:valueType="floatType"/>

                <objectAnimator
                    android:duration="216"
                    android:interpolator="@anim/interpolator_fingerprint_on_to_off_1"
                    android:propertyName="trimPathStart"
                    android:valueFrom="0.0"
                    android:valueTo="1.0"
                    android:valueType="floatType"/>
            </set>
        </aapt:attr>
    </target>
</animated-vector>
