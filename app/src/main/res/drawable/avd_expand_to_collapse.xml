<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/ic_expand">

    <target android:name="@string/expand_group_name">
        <aapt:attr name="android:animation">

            <objectAnimator
                android:duration="300"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="translateY"
                android:valueFrom="15"
                android:valueTo="9" />
        </aapt:attr>
    </target>

    <target android:name="@string/expand_left_group_name">
        <aapt:attr name="android:animation">

            <objectAnimator
                android:duration="250"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="rotation"
                android:valueFrom="135"
                android:valueTo="45" />
        </aapt:attr>
    </target>

    <target android:name="@string/expand_right_group_name">
        <aapt:attr name="android:animation">

            <objectAnimator
                android:duration="250"
                android:interpolator="@android:interpolator/fast_out_slow_in"
                android:propertyName="rotation"
                android:valueFrom="45"
                android:valueTo="135" />
        </aapt:attr>
    </target>
</animated-vector>
